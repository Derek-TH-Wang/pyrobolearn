        <sensor_topic>gps</sensor_topic>
        <horizontal_pos_std_dev>0.0</horizontal_pos_std_dev>
        <vertical_pos_std_dev>0.0</vertical_pos_std_dev>
        <update_rate>30</update_rate>
        <is_on>true</is_on>
        <enable_gazebo_messages>true</enable_gazebo_messages>
      </plugin>
    </sensor>
  </gazebo>
  <gazebo>
    <plugin filename="libcustom_battery_consumer_ros_plugin.so" name="eca_a9/gps_link_battery_consumer">
      <link_name>eca_a9/battery_link</link_name>
      <battery_name>eca_a9/battery</battery_name>
      <power_load>5</power_load>
      <topic_device_state></topic_device_state>
    </plugin>
  </gazebo>
  <!-- Sensor link -->
  <link name="eca_a9/cc_sensor_link">
    <inertial>
      <inertia ixx="0.00001" ixy="0.0" ixz="0.0" iyy="0.00001" iyz="0.0" izz="0.00001"/>
      <mass value="0.001"/>
      <!-- [kg] -->
      <origin rpy="0 0 0" xyz="0 0 0"/>
    </inertial>
  </link>
  <joint name="eca_a9/cc_sensor_joint" type="revolute">
    <origin rpy="0 0 0" xyz="0 0 0"/>
    <parent link="eca_a9/base_link"/>
    <child link="eca_a9/cc_sensor_link"/>
    <axis xyz="1 0 0"/>
    <limit effort="0" lower="0" upper="0" velocity="0"/>
  </joint>
  <gazebo>
    <plugin filename="libuuv_gazebo_ros_cpc_plugin.so" name="particle_concentration{suffix}_sensor">
      <robot_namespace>eca_a9</robot_namespace>
      <link_name>eca_a9/cc_sensor_link</link_name>
      <plume_topic>/plume/particles</plume_topic>
      <sensor_topic>particle_concentration</sensor_topic>
      <update_rate>10</update_rate>
      <gamma>0.001</gamma>
      <radius>3.0</radius>
      <enable_gazebo_messages>false</enable_gazebo_messages>
      <enable_local_ned_frame>false</enable_local_ned_frame>
      <static_reference_frame>world</static_reference_frame>
      <water_salinity_value>35.0</water_salinity_value>
      <plume_salinity_value>10.0</plume_salinity_value>
      <salinity_output_topic>salinity</salinity_output_topic>
      <salinity_unit>ppt</salinity_unit>
      <gain>1.0</gain>
      <noise_sigma>0</noise_sigma>
      <noise_amplitude>0</noise_amplitude>
    </plugin>
  </gazebo>
  <!-- Joint state publisher plugin -->
  <gazebo>
    <plugin filename="libjoint_state_publisher.so" name="joint_state_publisher">
      <robotNamespace>eca_a9</robotNamespace>
      <updateRate>50</updateRate>
    </plugin>
  </gazebo>
</robot>

